{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "share": "disabled",
  "autoupdate": false,
  // "small_model": "openrouter/google/gemini-2.5-flash-lite",
  "small_model": "openrouter/openai/gpt-oss-20b",

  "permission": {
    "bash": {
      "*": "allow",
      "git commit *": "ask",
      "git push *": "ask",
      "git clone *": "ask",
      "mv *": "ask",
      "rm *": "ask",
    },
  },

  "disabled_providers": [],
  "keybinds": {
    // "session_child_cycle": "ctrl+alt+right",
    // "session_child_cycle_reverse": "ctrl+alt+left",
  },

  "tui": {
    "scroll_speed": 5,
  },

  "formatter": {
    "gofmt": { "disabled": false },
    "mix": { "disabled": true },
    "prettier": { "disabled": false },
    "biome": { "disabled": true },
    "zig": { "disabled": true },
    "clang-format": { "disabled": true },
    "ktlint": { "disabled": true },
    "ruff": { "disabled": true },
    "rubocop": { "disabled": true },
    "standardrb": { "disabled": true },
    "htmlbeautifier": { "disabled": true },
  },

  "lsp": {
    // "typescript": { "disabled": true },
    "eslint": { "disabled": true },
    "gopls": { "disabled": true },
    "ruby-lsp": { "disabled": true },
    "pyright": { "disabled": true },
    "elixir-ls": { "disabled": true },
    "zls": { "disabled": true },
    "csharp": { "disabled": true },
    "vue": { "disabled": true },
    "rust": { "disabled": true },
    "clangd": { "disabled": true },
    "svelte": { "disabled": true },
  },

  "mcp": {
    "context7": {
      // "type": "remote",
      // "url": "https://mcp.context7.com/mcp",
      "enabled": true,
      "type": "local",
      "command": ["npx", "-y", "@upstash/context7-mcp"],
    },
    "figma-mcp": {
      "enabled": false,
      "type": "local",
      "command": ["/usr/bin/npx", "-y", "figma-developer-mcp", "--stdio"],
    },

    "chrome-devtools": {
      "enabled": false,
      "type": "local",
      "command": ["/usr/bin/npx", "-y", "chrome-devtools-mcp@latest"],
    },
  },

  "agent": {
    "creative": {
      "temperature": 1,
    },

    "build": {
      "temperature": 0.2,
    },
  },

  "provider": {
    "anthropic": {
      "models": {
        "sonnet-4-5-thinker": {
          "id": "claude-sonnet-4-5",
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 16000,
            },
          },
        },

        "claude-sonnet-4-5": {
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 5000,
            },
          },
        },

        "claude-sonnet-4-5-20250929": {
          "name": "Claude Sonnet 4.5 09-29",
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 5000,
            },
          },
        },

        "claude-haiku-4-5": {
          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 5000,
            },
          },
        },

        "claude-haiku-4-5-20251001": {
          "name": "Claude Haiku 4.5 10-01",

          "options": {
            "thinking": {
              "type": "enabled",
              "budgetTokens": 5000,
            },
          },
        },
      },
    },

    "google": {
      "options": {
        "apiKey": "{env:GEMINI_API_KEY}",
      },
    },

    "deepseek": {
      "options": {
        "apiKey": "{env:DEEPSEEK_API_KEY}",
      },
    },

    "openrouter": {
      // "npm": "@openrouter/ai-sdk-provider",

      "options": {
        "apiKey": "{env:OPENROUTER_API_KEY}",
      },

      "models": {
        "google/gemini-3-flash-preview": {
          "options": {
            "provider": {
              "only": ["google-ai-studio"],
            },
          },
        },

        "google/gemini-3-pro-preview": {
          "options": {
            "provider": {
              "only": ["google-ai-studio"],
            },
          },
        },

        "moonshotai/kimi-k2-0905": {
          "name": "MoonshotAI: Kimi K2 0905",

          "options": {
            "provider": {
              "only": ["moonshotai"],
            },
          },
        },

        "nvidia/llama-3.3-nemotron-super-49b-v1.5": {
          "name": "NVIDIA: Llama 3.3 Nemotron Super 49B V1.5",
        },

        "z-ai/glm-4-32b": {
          "name": "Z.AI: GLM 4 32B",
        },

        "z-ai/glm-4.6": {
          "options": {
            "provider": {
              "only": ["z-ai"],
            },
          },
        },

        "openai/gpt-oss-120b": {
          "options": {
            "provider": {
              "require_parameters": true,
              "only": ["groq"],
            },
          },
        },

        "openai/gpt-oss-20b": {
          "options": {
            "provider": {
              "only": ["groq"],
            },
          },
        },

        "openai/gpt-5-mini": {
          "options": {
            "provider": {
              "only": ["openai"],
            },
          },
        },

        "openai/gpt-5-nano": {
          "options": {
            "provider": {
              "only": ["openai"],
            },
          },
        },

        "openai/gpt-5": {
          "options": {
            "provider": {
              "only": ["openai"],
            },
          },
        },

        "openai/gpt-5-mini-high": {
          "id": "openai/gpt-5-mini",
          "name": "GPT-5 Mini (High)",

          "options": {
            "reasoningEffort": "high",
          },
        },

        "openai/gpt-5-nano-high": {
          "id": "openai/gpt-5-nano",
          "name": "GPT-5 Nano (High)",

          "options": {
            "reasoningEffort": "high",
          },
        },

        "google/gemini-2.5-flash-preview-09-2025": {
          "cost": {
            "cache_read": 0.075,
            "cache_write": 0.383,
            "input": 0.3,
            "output": 2.5,
          },

          "options": {
            "reasoning": {
              "max_tokens": 2000,
            },
          },
        },

        "google/gemini-2.5-flash": {
          "options": {
            "reasoning": {
              "max_tokens": 2000,
            },
            "provider": {
              "only": ["google-vertex/global"],
            },
          },
        },

        "google/gemini-2.5-flash-lite-reasoning": {
          "id": "google/gemini-2.5-flash-lite",
          "name": "Google: Gemini 2.5 Flash Lite (Reasoning)",
          "options": {
            "reasoning": {
              "max_tokens": 2000,
            },
          },
        },

        "qwen/qwen3-coder-30b-a3b-instruct": {
          "name": "Qwen: Qwen3 Coder 30B A3B Instruct",
          "options": {
            "provider": {
              "ignore": ["alibaba"],
              "require_parameters": true,
            },
          },
        },

        "qwen/qwen3-coder-flash": {
          "name": "Qwen: Qwen3 Coder Flash",

          "cost": {
            "input": 0.5,
            "output": 1.5,
            "cache_read": 0.12,
          },
        },

        "x-ai/grok-4-fast": {
          "name": "xAI: Grok 4 Fast",
          "cost": {
            "input": 0.2,
            "output": 0.4,
            "cache_read": 0.05,
          },
        },

        "deepseek/deepseek-v3.2-exp": {
          "name": "DeepSeek: DeepSeek V3.2 Exp",

          "cost": {
            "input": 0.27,
            "output": 0.4,
          },

          "options": {
            "provider": {
              "only": ["novita/fp8", "deepinfra"],
            },
          },
        },

        "deepseek/deepseek-v3.1-terminus": {
          "options": {
            "provider": {
              "only": ["novita", "deepinfra/fp4"],
            },
          },
        },
      },
    },
  },
}

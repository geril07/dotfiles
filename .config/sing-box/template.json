{
  "log": {
    "level": "info",
    "timestamp": true,
    "output": "/tmp/box.log"
  },
  "dns": {
    "servers": [
      {
        "tag": "cloudflare",
        "type": "tls",
        "server": "1.1.1.1"
      },

      {
        "tag": "google",
        "type": "tls",
        "server": "8.8.8.8"
      }
    ],
    "final": "google",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "proxy-tun",
      "mtu": 1500,
      "address": "172.19.0.1/24",
      "auto_route": true,
      "strict_route": true,
      "auto_redirect": true,
      "stack": "mixed"
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },

      {
        "rule_set": ["geoip-private", "geoip-ru"],
        "outbound": "direct"
      },

      {
        "process_path": ["/usr/lib/firefox/firefox"],
        "outbound": "proxy-work"
      },

      {
        "process_path": [
          "/usr/bin/dnf",
          "/usr/bin/dnf5",
          "/usr/bin/pacman",
          "/usr/bin/yay",
          "/home/geril/.local/bin/hf",
          "/home/geril/.local/bin/huggingface-cli"
        ],
        "outbound": "direct"
      },

      {
        "domain_keyword": [
          "godeaf",
          "gencit",
          "opravar",
          "allo.cdnlbox.club",
          "obrut.show",
          "curseforge",
          "forgecdn",
          "stream-balancer-allo",
          "anime",
          "kodik",

          "playlist.ttvnw",
          "hls.ttvnw"
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "format": "binary",
        "tag": "geoip-ru",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/ru.srs"
      },
      {
        "format": "binary",
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs"
      },
      {
        "format": "binary",
        "tag": "geoip-private",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/private.srs"
      }
    ],
    "final": "proxy",
    "find_process": true,
    "auto_detect_interface": true,
    "default_domain_resolver": "google"
  },

  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db"
    }
  }
}

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " {{.Icon}}",
          "foreground": "#45F1C2",
          "background": "transparent",
          "type": "os",
          "style": "plain"
        },
        {
          "template": " {{.UserName}} on",
          "foreground": "#45F1C2",
          "type": "session",
          "style": "plain"
        },
        {
          "options": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": " \uf07b {{ .Path }} ",
          "foreground": "#0CA0D8",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "fetch_upstream_icon": true
          },
          "template": "{{ if .UpstreamIcon }}{{ .UpstreamIcon }} {{ end }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#7a7a7a></>",
          "foreground": "#6CA35E",
          "type": "node",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738 {{ .Full }} ",
          "foreground": "#ffffff",
          "background": "#4c1f5e",
          "type": "java",
          "style": "diamond"
        },
        {
          "options": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}<#ff0000>\uf00d \ue23a</>{{ else }}<#23d18b>\uf42e \ue23a</>{{ end }} ",
          "type": "status",
          "style": "plain"
        },
        {
          "options": {
            "threshold": 10
          },
          "template": " took \uf252 {{ .FormattedMs }} ",
          "foreground": "#bab02a",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "options": {
            "time_format": "15:04:05"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#00C5C7",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " ‚ùØ",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}

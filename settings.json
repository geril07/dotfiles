{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.colorCustomizations": {
    "[One Dark Pro]": {
      "editorBracketHighlight.foreground1": "#FFD700",
      "editorBracketHighlight.foreground2": "#c678dd",
      "editorBracketHighlight.foreground3": "#56b6c2"
    }
  },
  "workbench.colorTheme": "One Dark Pro",
  "explorer.decorations.colors": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "'JetBrainsMono NF', monospace",
  "terminal.integrated.fontSize": 12,
  "window.newWindowDimensions": "maximized",
  "emmet.triggerExpansionOnTab": true,
  "prettier.useEditorConfig": false,
  "prettier.printWidth": 100,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "vsintellicode.modify.editor.suggestSelection": "enabled",
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.enabled": false,
  "eslint.alwaysShowStatus": true,
  "vsicons.dontShowNewVersionMessage": true,
  "reactSnippets.settings.importReactOnTop": true,
  "reactSnippets.settings.typescriptPropsStatePrefix": "interface",
  "rust-analyzer.completion.callable.snippets": "none",
  "files.associations": {
    "*.cgi": "php"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  // "vim.cursorStylePerMode.insert": "block",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["<C-c>"],
      "after": ["y"]
    }
  ],
  "vim.overrideCopy": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["<Esc>"],
      "after": ["<Esc>"],
      "commands": ["workbench.action.files.save"],
      "silent": true
    },
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-c"],
      "after": []
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    // { "before": ["u"], "commands": ["undo"] },
    { "before": ["K"], "commands": ["lineBreakInsert"], "silent": true },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    },
    { "before": ["<C-n>"], "commands": [":noh"] },
    { "before": ["g", "f"], "commands": ["editor.action.goToReferences"] },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prev", "closeMarkersNavigation", "editor.action.showHover"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.marker.next", "closeMarkersNavigation", "editor.action.showHover"]
    },
    { "before": ["<leader>", "d"], "commands": ["editor.action.revealDefinition"] },
    { "before": ["<leader>", "b"], "commands": ["workbench.action.navigateBack"] },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    { "before": ["<leader>", "t"], "commands": ["workbench.action.showAllSymbols"] },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.openSettingsJson"]
    },
    {
      "before": ["<leader>", "f", "z"],
      "commands": ["search.action.openNewEditor"]
    },
    { "before": ["<leader>", "x", "e", "E"], "commands": ["errorLens.toggle"] },
    {
      "before": ["<leader>", "x", "e", "e"],
      "commands": ["errorLens.toggleError"]
    },
    {
      "before": ["<leader>", "x", "e", "w"],
      "commands": ["errorLens.toggleWarning"]
    },
    { "before": ["<leader>", "x", "e", "i"], "commands": ["errorLens.toggleInfo"] },
    {
      "before": ["<leader>", "a", "f"],
      "commands": ["workbench.action.toggleCenteredLayout"]
    },
    {
      "before": ["<leader>", "a", "a"],
      "commands": ["workbench.action.toggleActivityBarVisibility"]
    },
    {
      "before": ["<leader>", "<leader>", "W"],
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "w"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-r>": false,
    "<C-w>": false,
    "<C-f>": false,
    "<C-k>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-h>": false,
    "<C-j>": false,
    "<C-t>": false
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "zenMode.hideLineNumbers": false,
  "errorLens.enabledDiagnosticLevels": ["info", "warning", "error"],
  "errorLens.enabled": true,
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.messageBackgroundMode": "none",
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.hidesExplorerArrows": false,
  "background.backgroundOpacity": [0.93, 0.9, 0.9, 0.9],
  "settingsSync.ignoredSettings": ["background.windowBackgrounds"],
  "files.autoSave": "onFocusChange",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*.mutable": {
        "underline": false
      }
    }
  },
  "editor.suggestOnTriggerCharacters": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "background.windowBackgrounds": ["c:/background/54249-devushka_anime_volosy.jpg"],
  "editor.stickyScroll.enabled": false,
  "editor.fontSize": 14,
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 4,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.fontLigatures": true,
  "editor.smoothScrolling": false,
  "editor.cursorBlinking": "solid",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.suggestSelection": "first",
  "editor.fontFamily": "JetBrains Mono",
  "editor.wordWrap": "on",
  "editor.occurrencesHighlight": false,
  "editor.linkedEditing": true,

  "editor.bracketPairColorization.enabled": true,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "none",
  "update.mode": "none",
  "update.enableWindowsBackgroundUpdates": false,
  "window.zoomLevel": 1,
  "editor.unicodeHighlight.ambiguousCharacters": false
}

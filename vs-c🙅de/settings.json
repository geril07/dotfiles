{
  "editor.smoothScrolling": true,
  "eslint.enable": false,
  "typescript.validate.enable": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.colorCustomizations": {
    "[One Dark Pro]": {
      "editorBracketHighlight.foreground1": "#FFD700",
      "editorBracketHighlight.foreground2": "#c678dd",
      "editorBracketHighlight.foreground3": "#56b6c2"
    }
  },
  "catppuccin.boldKeywords": false,
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "explorer.decorations.colors": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', monospace",
  "terminal.integrated.fontSize": 18,
  "window.newWindowDimensions": "maximized",
  "emmet.triggerExpansionOnTab": false,
  "prettier.useEditorConfig": false,
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.useTabs": false,
  "prettier.tabWidth": 2,
  "prettier.semi": false,
  "vsintellicode.modify.editor.suggestSelection": "enabled",
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.enabled": false,
  "reactSnippets.settings.importReactOnTop": false,
  "reactSnippets.settings.typescriptPropsStatePrefix": "interface",
  "rust-analyzer.completion.callable.snippets": "none",
  "rust-analyzer.rustfmt.extraArgs": ["--config", "tab_spaces=2"],
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "vim.halfPageScrollCentered": true,
  "vim.surround": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": false,
  "vim.visualModeKeyBindings": [
    {
      "before": ["<C-c>"],
      "after": ["\"", "+", "y"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<Esc>"],
      "after": ["<Esc>"],
      "commands": ["workbench.action.files.save"],
      "silent": true
    },
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-c>"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["Y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    {
      "before": ["M"],
      "after": ["`"],
      "silent": true
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["[", "e"],
      "commands": [
        "editor.action.marker.prev",
        "closeMarkersNavigation",
        "editor.action.showHover"
      ]
    },
    {
      "before": ["]", "e"],
      "commands": [
        "editor.action.marker.next",
        "closeMarkersNavigation",
        "editor.action.showHover"
      ]
    },
    {
      "before": ["g", "f"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.format"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.openSettingsJson"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "x", "e", "E"],
      "commands": ["errorLens.toggle"]
    },
    {
      "before": ["<leader>", "x", "e", "e"],
      "commands": ["errorLens.toggleError"]
    },
    {
      "before": ["<leader>", "x", "e", "w"],
      "commands": ["errorLens.toggleWarning"]
    },
    {
      "before": ["<leader>", "x", "e", "i"],
      "commands": ["errorLens.toggleInfo"]
    },
    {
      "before": ["<leader>", "a", "f"],
      "commands": ["workbench.action.toggleCenteredLayout"]
    },
    {
      "before": ["<leader>", "a", "a"],
      "commands": ["workbench.action.toggleActivityBarVisibility"]
    },
    {
      "before": ["<leader>", "a", "p"],
      "commands": ["workbench.action.pinEditor"]
    },
    {
      "before": ["<leader>", "a", "b"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "<leader>", "W"],
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "w"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-r>": false,
    "<C-w>": true,
    "<C-f>": false,
    "<C-k>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-h>": false,
    "<C-j>": false,
    "<C-t>": false
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "zenMode.hideLineNumbers": false,
  "errorLens.enabledDiagnosticLevels": ["info", "warning", "error"],
  "errorLens.enabled": true,
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.messageBackgroundMode": "none",
  "errorLens.editorHoverPartsEnabled": {
    "buttonsEnabled": false
  },
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.hidesExplorerArrows": false,
  "background.backgroundOpacity": [0.95, 0.9, 0.9, 0.9],
  "settingsSync.ignoredSettings": [
    "background.windowBackgrounds",
    "background.backgroundOpacity",
    "editor.smoothScrolling",
    "eslint.enable",
    "editor.fontSize"
  ],
  "files.autoSave": "onFocusChange",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*.mutable": {
        "underline": false
      }
    }
  },
  "editor.tokenColorCustomizations": {
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "scope": [
            "meta.type.tuple.ts entity.name.label.ts",
            "meta.type.tuple.tsx entity.name.label.tsx"
          ],
          "settings": {
            "foreground": "#E06C75"
          }
        }
      ]
    }
  },
  "editor.suggestOnTriggerCharacters": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.stickyScroll.enabled": false,
  "editor.fontSize": 15.5,
  "editor.fontWeight": 500,
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 10,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "solid",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.suggestSelection": "first",
  "editor.fontFamily": "JetBrains Mono",
  "editor.wordWrap": "on",
  "editor.occurrencesHighlight": "off",
  "editor.linkedEditing": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "none",
  "update.mode": "none",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "One Dark Pro",
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "window.titleBarStyle": "native",
  "window.menuBarVisibility": "toggle",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1,
    "vscodevim.vim": 1
  },
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "tailwindCSS.classFunctions": [
    "cn",
    "cva",
    "cvaWithVariants",
    "cvaWithVariantsAndDefault",
    "twMerge",
    "twJoin"
  ]
}
